{
    "name": "@alex/ts",
    "version": "0.0.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./base": "./dist/typescript.base.json",
        "./fastify": "./dist/typescript.fastify.json",
        "./gatsby": "./dist/typescript.gatsby.json",
        "./nestjs": "./dist/typescript.nestjs.json",
        "./nextjs": "./dist/typescript.nextjs.json",
        "./node": "./dist/typescript.node.json",
        "./react": "./dist/typescript.react.json",
        "./vite": "./dist/typescript.vite.json"
    },
    "files": [
        "dist",
        "./dist/typescript.base.json",
        "./dist/typescript.fastify.json",
        "./dist/typescript.gatsby.json",
        "./dist/typescript.nestjs.json",
        "./dist/typescript.nextjs.json",
        "./dist/typescript.node.json",
        "./dist/typescript.react.json",
        "./dist/typescript.vite.json"
    ],
    "typesVersions": {
        "*": {
            "base": [
                "./dist/typescript.base.json"
            ],
            "fastify": [
                "./dist/typescript.fastify.json"
            ],
            "gatsby": [
                "./dist/typescript.gatsby.json"
            ],
            "nestjs": [
                "./dist/typescript.nestjs.json"
            ],
            "nextjs": [
                "./dist/typescript.nextjs.json"
            ],
            "node": [
                "./dist/typescript.node.json"
            ],
            "react": [
                "./dist/typescript.react.json"
            ],
            "vite": [
                "./dist/typescript.vite.json"
            ]
        }
    },
    "license": "MIT",
    "engines": {
        "node": ">=20.18.2"
    },
    "packageManager": "yarn@4.6.0",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "yarn build:clean && yarn check-types && node esbuild.config.js",
        "build:clean": "rimraf dist",
        "clean": "rimraf node_modules",
        "clean:all": "rm -rf node_modules dist",
        "test": "vitest",
        "check-types": "tsc --noEmit",
        "test:watch": "vitest --watch",
        "format:write": "biome format --write",
        "lint:write": "biome lint --write",
        "check:write": "biome check --write"
    },
    "dependencies": {
        "typescript": "^5.7.3"
    },
    "peerDependencies": {
        "typescript": "^5.7.3"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@types/node": "^22.13.1",
        "esbuild": "^0.25.0",
        "oxc-transform": "^0.48.2",
        "rimraf": "^6.0.1",
        "vitest": "^3.0.5"
    }
}
